{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "lto-playground": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "lto",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/lto-playground",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              {
                "glob": "**/*",
                "input": "./node_modules/monaco-editor/dev/vs",
                "output": "libs/monaco/vs"
              }
            ],
            "styles": ["src/styles.scss"],
            "scripts": [
              "./node_modules/vis/dist/vis.min.js",
              "./node_modules/vis/dist/vis-network.min.js",
              "./node_modules/d3/dist/d3.min.js",
              "./node_modules/d3-zoom/build/d3-zoom.min.js",
              "./node_modules/d3-scale/dist/d3-scale.min.js",
              "./node_modules/lodash/lodash.min.js",
              "./node_modules/graphlib/dist/graphlib.core.js",
              "./node_modules/dagre/dist/dagre.core.js",
              "./node_modules/dagre-d3/dist/dagre-d3.core.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "lto-playground:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "lto-playground:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "lto-playground:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": ["src/styles.scss"],
            "scripts": [],
            "assets": ["src/favicon.ico", "src/assets"]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["src/tsconfig.app.json", "src/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "lto-playground-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "lto-playground:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "lto-playground:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "waffle": {
      "root": "projects/waffle",
      "sourceRoot": "projects/waffle/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/waffle/tsconfig.lib.json",
            "project": "projects/waffle/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "projects/waffle/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/waffle/src/test.ts",
            "tsConfig": "projects/waffle/tsconfig.spec.json",
            "karmaConfig": "projects/waffle/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["projects/waffle/tsconfig.lib.json", "projects/waffle/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "lto-forms": {
      "root": "projects/lto-forms",
      "sourceRoot": "projects/lto-forms/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/lto-forms/tsconfig.lib.json",
            "project": "projects/lto-forms/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "projects/lto-forms/ng-package.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/lto-forms/src/test.ts",
            "tsConfig": "projects/lto-forms/tsconfig.spec.json",
            "karmaConfig": "projects/lto-forms/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/lto-forms/tsconfig.lib.json",
              "projects/lto-forms/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    }
  },
  "defaultProject": "lto-playground"
}
